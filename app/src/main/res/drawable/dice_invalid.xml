<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/diceDrawableColorID">
        <color android:color="@android:color/transparent"/>
    </item>
    <item>
        <rotate android:fromDegrees="45"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="45">
            <shape android:shape="line">
                <stroke android:width="2dp" android:color="#808080" />
            </shape>
        </rotate>
    </item>
    <item>
        <rotate android:fromDegrees="135"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="135">
            <shape android:shape="line">
                <stroke android:width="2dp" android:color="#808080" />
            </shape>
        </rotate>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:color="#922b21" android:width="3dp" />
            <size android:width="220dp" android:height="220dp"/>
        </shape>
    </item>
</layer-list>